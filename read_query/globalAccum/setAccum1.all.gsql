CREATE OR REPLACE QUERY gSetAccum101 () {
  SetAccum<VERTEX> @@set1;
  SetAccum<VERTEX> @@set2;
  SetAccum<VERTEX> @@set;

  SetAccum<VERTEX> @@setA;
  SetAccum<VERTEX> @@setB;
  SetAccum<VERTEX> @@setC;
  SetAccum<VERTEX> @@setD;
  SetAccum<VERTEX> @@setE;
  SetAccum<VERTEX> @@setF;

  L1 = SELECT s FROM Person:s
     WHERE s.firstName == "John" AND s.lastName == "Reddy"
     ACCUM @@set1 += s;
       
  L1 = SELECT s FROM Person:s
     WHERE s.gender == "male"
     ACCUM @@set1 += s;
        
  L1 = SELECT s FROM Person:s
     WHERE s.firstName == "John" AND s.lastName == "Reddy"
     ACCUM
       @@set += s.neighbors(),
       @@setA += @@set1 MINUS s.neighbors(),
       @@setB += @@set1 INTERSECT s.neighbors(),
       @@setC += @@set1 UNION s.neighbors(),
       @@setD += s.neighbors() MINUS @@set1,
       @@setE += s.neighbors() INTERSECT @@set1,
       @@setF += s.neighbors() UNION @@set1;

   PRINT @@set1;
   PRINT @@set2;
   PRINT @@set1 UNION @@set2;
   PRINT @@set2 INTERSECT @@set1;
   PRINT @@set2 MINUS @@set1;
   
   PRINT @@set;
   PRINT @@setA;
   PRINT @@setB;
   PRINT @@setC;
   PRINT @@setD;
   PRINT @@setE;
   PRINT @@setF;
}
